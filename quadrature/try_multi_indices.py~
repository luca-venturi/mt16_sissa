import numpy as np
from multi_indices import *

d = 5
q = 8

def d_tuples(d, q):
        
    k = np.ones((1,d))
    khat = (q-d+1)*k
    ind = np.zeros((1,d))
    count = 0
       
    p = 0
    while k[0,d-1] <= q:
        k[0,p] = k[0,p]+1
        if k[0,p] > khat[0,p]:
            if p != d-1:
                k[0,p] = 1
                p = p+1
        else:
            for j in range(p):
                khat[0,j] = khat[0,p]-k[0,p]+1
            k[0,0] = khat[0,0]
            p = 0
            count = count +1
            ind = np.concatenate((ind,k))
        
    return ind, count

ind, count = d_tuples(d,q)

print str(count) 
